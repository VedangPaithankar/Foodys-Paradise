{
  "project_name": "Foodys-Paradise",
  "frontend": {
    "git_directory": "FoodysParadise - FrontEnd",
    "type": "node",
    "build_command": "npm install",
    "start_command": "npm start",
    "env": {
      "PORT": "DYNAMIC"
    }
  },
  "backend": {
    "git_directory": "FoodysParadise - BackEnd",
    "type": "node",
    "build_command": "npm install",
    "start_command": "npm start",
    "env": {
      "PORT": "DYNAMIC",
      "MONGODB_URI": "mongodb://localhost:27017/Foodys-Paradise"
    }
  },
  "dependencies": [
    "nodejs",
    "npm",
    "mongodb",
    "git",
    "net-tools",
    "jq"
  ]
}
