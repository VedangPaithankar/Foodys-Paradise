{
  "project_name": "Foodys-Paradise",
  "type": "mern",
  "frontend": {
    "git_directory": "FoodysParadise - FrontEnd",
    "directory": "client",
    "build_command": "npm install && npm start",
    "env": {
      "PORT": "DYNAMIC"
    }
  },
  "backend": {
    "git_directory": "FoodysParadise - BackEnd",
    "directory": "server",
    "build_command": "npm install && npm start",
    "env": {
      "PORT": "DYNAMIC",
      "MONGODB_URI": "mongodb://localhost:27017/Foodys-Paradise"
    }
  },
  "dependencies": [
    "nodejs",
    "npm",
    "mongodb-org",
    "sqlite3",
    "git",
    "net-tools",
    "jq"
  ]
}
